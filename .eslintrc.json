{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "camelcase": ["error"],
        "eqeqeq": ["error", "always"],
        "no-console": "off"
    },
    //"parserOptions": {
      //  "sourceType": "module"
    //}
}
